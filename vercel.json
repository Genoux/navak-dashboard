{
  "version": 2,
  "builds": [
    {
      "src": "bundle/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bundle/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ROOT_URL": "https://navak-dashboard.vercel.app",
    "DEMO_MODE": "true",
    "MONGO_URL": "mongodb://localhost:27017/demo"
  }
}